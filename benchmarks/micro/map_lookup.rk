func fill_map() -> Map<i64, i64> {
    let m = Map.new()
    let i = 0
    while i < 10000 {
        m.insert(i, i * 2)
        i = i + 1
    }
    return m
}

benchmark "map lookup 10k" {
    let m = fill_map()
    let sum = 0
    let i = 0
    while i < 10000 {
        sum = sum + m.get(i)
        i = i + 1
    }
}
